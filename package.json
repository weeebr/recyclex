{
  "name": "pickup-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.2",
    "autosuggest-highlight": "^3.1.1",
    "date-fns": "^2.0.0-beta.3",
    "eslint": "^6.1.0",
    "firebase": "^6.3.4",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "eslint:recommended"
    ],
    "env": {
      "node": false,
      "es6": true,
      "browser": true
    },
    "rules": {
      "semi": "error",
      "no-unused-vars": "off",
      "func-names": "off",
      "indent": "off",
      "no-else-return": "off",
      "prefer-arrow-callback": "off",
      "no-undef": "off",
      "no-use-before-define": "off",
      "comma-dangle": "off",
      "eol-last": "off",
      "no-trailing-spaces": "off",
      "linebreak-style": "off",
      "no-console": "off",
      "no-restricted-globals": "off",
      "object-shorthand": "off",
      "no-shadow": "off",
      "no-debugger": "off",
      "prefer-const": "off",
      "no-multiple-empty-lines": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
